{
  "compilerOptions": {
    "jsx": "react-jsx",
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "types": ["vitest/globals"]
  },
  "include": ["vite.config.ts", "vitest.config.ts", "vitest.setup.ts", "__tests__/**/*", "src/**/*"]
}
